<h1>Add namespace</h1>

<% form_for :namespace, :url => { :action => "create" }, :html => { :class => 'form' } do |f| %>
  <%= f.error_messages %>

  <%= f.label :prefix %>
  <p>
    <%= f.text_field 'prefix' %>
  </p>

	<%= f.label :uri %>
  <p>
    <%= f.text_field 'uri' %>
  </p>
	
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', :action => :index %>