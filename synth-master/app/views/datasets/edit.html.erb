<h1>Edit dataset</h1>

<% form_for :dataset, :url => { :action => "update" }, :html => { :class => 'form' } do |f| %>
  <%= f.error_messages %>
  <%= hidden_field_tag :id, @dataset %>
  <%= f.label :title %>
  <p>
    <%= f.text_field :title %>
  </p>

	<%= f.label :SPARQL_Endpoint_URI %>
	<p>
    <%= f.text_field :sparqlEndpoint %>
  </p>
	
  <p>
    <%= f.submit 'Save' %>
  </p>
<% end %>

<%= link_to 'Back', concrete_interfaces_path %>