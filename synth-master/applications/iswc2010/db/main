    <dc:title>Hybrid Graph based Keyword Query Interpretation on RDF</dc:title>
    <bibo:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2010/paper/520/authorlist"/>
    <swrc:url rdf:resource="http://iswc2010.semanticweb.org/pdf/520.pdf"/>
