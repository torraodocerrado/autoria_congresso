    <dc:title>Making sense of Twitter</dc:title>
    <swrc:url rdf:resource="http://iswc2010.semanticweb.org/pdf/352.pdf"/>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/david-laniado"/>
