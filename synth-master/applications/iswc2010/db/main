    <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
    <dc:title>Linking and Building Ontologies of Linked Data</dc:title>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/rahul-parundekar"/>
