  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2010/paper/305">
    <rdfs:label>SAOR: Template Rule Optimisations for Distributed Reasoning over 1 Billion Linked Data Triples</rdfs:label>
    <dc:title>SAOR: Template Rule Optimisations for Distributed Reasoning over 1 Billion Linked Data Triples</dc:title>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/aidan-hogan"/>
    <dc:subject>semantic web</dc:subject>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/stefan-decker"/>
