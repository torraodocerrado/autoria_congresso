               attr = [ attribute.label ]&#xD;
             end&#xD;
           end &#xD;
           %&gt;&#xD;
            &lt;td&gt;&#xD;
              &lt;ul&gt;&#xD;
                &lt;% for attribute_value in attr do %&gt;&lt;li&gt;&lt;%= attribute_value %&gt;&lt;/li&gt;&lt;% end %&gt;&#xD;
              &lt;/ul&gt;&#xD;
   	        &lt;/td&gt;&#xD;
