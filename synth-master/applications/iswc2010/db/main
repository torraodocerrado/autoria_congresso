  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2010/paper/83">
    <dc:creator rdf:resource="http://data.semanticweb.org/person/frank-van-harmelen"/>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/christophe-gueret"/>
    <dc:subject>semantic web</dc:subject>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/stefan-schlobach"/>
    <bibo:authorList rdf:resource="http://data.semanticweb.org/conference/iswc/2010/paper/83/authorlist"/>
