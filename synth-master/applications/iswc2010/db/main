    <rdfs:label>Using Semantics for Automating the Authentication of Web APIs</rdfs:label>
    <dc:title>Using Semantics for Automating the Authentication of Web APIs</dc:title>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/ivan-martinez"/>
    <dc:subject>semantic web</dc:subject>
