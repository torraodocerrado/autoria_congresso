  <rdf:Description rdf:about="http://data.semanticweb.org/conference/iswc/2010/paper/218">
    <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/iswc/2010/proceedings"/>
    <foaf:maker rdf:resource="http://data.semanticweb.org/person/kunal-verma"/>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/amit-sheth"/>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/kunal-verma"/>
