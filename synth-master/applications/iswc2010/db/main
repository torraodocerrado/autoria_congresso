    <dc:subject>Linked Data</dc:subject>
    <dc:creator rdf:resource="http://data.semanticweb.org/person/daniel-schwabe"/>
    <dc:title>Building Linked Data Applications with Fusion: A Visual Interface for Exploration and Mapping</dc:title>
