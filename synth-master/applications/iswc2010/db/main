  $('#input_filter').keyup(function() {&#xD;
     var value = $(this).val().trim();&#xD;
     if(value == ""){&#xD;
       $('.entry').show();            &#xD;
     }&#xD;
     else&#xD;
    {&#xD;
      var re = new RegExp(value,"i");&#xD;
      $('.entry').hide()&#xD;
