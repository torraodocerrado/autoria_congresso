if (swui_piece(property, "|", 1) != "") {
// this is an index's entry
// we have to look for the object's id in jsonData
nodeWidg = nodeWidMap[swui_piece(swui_id, "$", 0,1)];
seq = parseInt(swui_piece(swui_piece(swui_id, "$", swui_piece(swui_id, "$")-1),"s",1));
interf = inteWidMap[nodeWidg];
nodesArray = parseJSONResponse(readJSONData(interf));
if (nodesArray != "") {
  count = 0;
  for (var i = 0, len = nodesArray.length; i &lt; len; ++i) {
     traverseJSON(nodesArray[i]);
  }
}
} else {
// swui_id variable holds the object's id
objId = swui_piece(swui_piece(swui_id, "$", 1, 2), "id", 1);
callNewAjaxInPlaceEditor();
}

function traverseJSON(elem) {
  for (var attrName in elem) {
    if (typeof elem[attrName] == "object") { traverseJSON(elem[attrName]) } 
    else {
        if (elem[attrName]!="") { 
            ptyPfx = swui_piece(attrName, "|", 0, swui_piece(attrName, "|")-1)
            ptySfx = swui_piece(attrName, "|", swui_piece(attrName, "|")-1)
            if (ptyParent ==  ptyPfx &amp;&amp; (ptySfx=="swui:id") ) { 
              if (seq == count) {
                 objId = elem[attrName]; 
                 callNewAjaxInPlaceEditor();
              }
              count=count+1;
            }
        }
    }
  }
}

function callNewAjaxInPlaceEditor() {
   new Ajax.InPlaceEditor(editBoxRef, '/execute/change_property/'+encodeURIComponent(objId), {callback: function(form, value) { propertyName=setPropertyName(); str='property=' + propertyName + '&amp;value=' + encodeURIComponent(value);return str}}, {rows:1,cols:40});
}

function setPropertyName() {
 modelRef = document.getElementById(swui_getRunTimeRef(undefined, swui_id)); 
 property = modelRef.getAttribute("property"); 
 if (swui_piece(property, "|", 1) != "") {
   propertyName=swui_piece(property, "|", swui_piece(property, "|")-1)
 } else {
   propertyName=property
 } 
 return propertyName; 
}</swui:jsCode>
