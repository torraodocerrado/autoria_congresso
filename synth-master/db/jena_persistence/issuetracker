                  &lt;tr class=&lt;%= cycle('', '"even"') %&gt;&gt;&#xD;
                   &lt;% for attribute_name in entry.attributes_names do &#xD;
                         attribute = entry.attributes_hash[attribute_name]&#xD;
                         if attribute.label.is_a?(Array)&#xD;
                              if attribute.respond_to?(:target_context)&#xD;
                                 attr = attribute.label.map{|o| link_to(o[:label], attribute.target_context.url(attribute.target_parameters, o[:target_url]), :class =&gt; attribute_name)}&#xD;
                              else&#xD;
                                attr = attribute.label.map{|o| link_to(o[:label], SHDM::Context.find_by.context_name('OneResource').execute.first.url({}, o[:target_url]), :class =&gt; attribute_name)}&#xD;
                              end&#xD;
                         else                            &#xD;
                           attr = []&#xD;
                           if attribute.respond_to?(:target_url)&#xD;
                             attr &lt;&lt; link_to(attribute.label, attribute.target_url, :rel =&gt; attribute_name)&#xD;
                           else&#xD;
                             attr &lt;&lt; attribute.label&#xD;
                           end&#xD;
                        end %&gt; &#xD;
                      &lt;td&gt;&lt;ul&gt;&lt;% for attribute_value in attr do %&gt;&lt;li style="list-style-type: none"&gt;&lt;%= attribute_value %&gt;&lt;/li&gt;&lt;% end %&gt;&lt;/ul&gt;&lt;/td&gt;&#xD;
                  &lt;% end %&gt;&#xD;
                  &lt;/tr&gt;&#xD;
                &lt;% end %&gt;&#xD;
