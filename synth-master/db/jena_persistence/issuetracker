    <dc:title>Mashup: using tabulator in another page in the web</dc:title>
    <rdf:type rdf:resource="http://dig.csail.mit.edu/2010/issues/track#Feature"/>
    <wf:message rdf:resource="http://tabulator.org/wiki/tabulator/tracker/msg#msg70"/>
