    <shdm:navigation_attribute_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">label</shdm:navigation_attribute_name>
    <shdm:computed_navigation_attribute_value_expression rdf:datatype="http://www.w3.org/2001/XMLSchema#string">l = self.rdfs::label
