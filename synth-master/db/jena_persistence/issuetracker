              &lt;table class="table table-autosort:0 table-stripeclass:even table-autofilter"&gt;&#xD;
                &lt;thead&gt;&#xD;
                &lt;tr&gt;&#xD;
                  &lt;% for attribute in @index.index_attributes.sort{|a,b| a.navigation_attribute_index_position.first &lt;=&gt; b.navigation_attribute_index_position.first} do %&gt;&#xD;
                  &lt;th class="table-sortable:ignorecase table-sortable"&gt;&lt;%= attribute.navigation_attribute_name %&gt;&lt;/th&gt;&#xD;
                  &lt;% end %&gt;&#xD;
