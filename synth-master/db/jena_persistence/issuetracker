&lt;body&gt;&#xD;
  &lt;div id="container"&gt;&#xD;
    &lt;div id="header"&gt;&#xD;
      &lt;h1&gt;&lt;a href="/execute/init"&gt;Semantic Issue Tracker&lt;/a&gt;&lt;/h1&gt;&#xD;
      &#xD;
      &lt;%= include 'DefaultLandmarksConcrete' %&gt;&#xD;
    &lt;/div&gt;&#xD;
    &lt;div id="wrapper" class="wat-cf"&gt;&#xD;
      &lt;div id="main"&gt;&#xD;
        &lt;div class="block" id="block-tables"&gt;&#xD;
          &lt;div class="content"&gt;&#xD;
            &lt;h2 class="title"&gt;&lt;%= @index.index_title %&gt;&lt;/h2&gt;&#xD;
            &lt;div class="inner"&gt;&#xD;
              &lt;table class="table table-autosort:0 table-stripeclass:even table-autofilter"&gt;&#xD;
                &lt;thead&gt;&#xD;
                &lt;tr&gt;&#xD;
                  &lt;% for attribute in @index.index_attributes.sort{|a,b| a.navigation_attribute_index_position.first &lt;=&gt; b.navigation_attribute_index_position.first} do %&gt;&#xD;
                  &lt;th class="table-sortable:ignorecase table-sortable"&gt;&lt;%= attribute.navigation_attribute_name %&gt;&lt;/th&gt;&#xD;
                  &lt;% end %&gt;&#xD;
                &lt;/tr&gt;&#xD;
                &lt;tr&gt;&#xD;
                   &lt;% @index.index_attributes.to_a.each do %&gt;&#xD;
                     &lt;th&gt;&lt;input name="filter" onkeyup="Table.filter(this,this)" style="width: inherit;"&gt;&lt;/th&gt;&#xD;
                   &lt;% end %&gt;&#xD;
                &lt;/tr&gt;&#xD;
                &lt;/thead&gt;&#xD;
                &lt;tbody&gt;&#xD;
                &lt;% for entry in @index.entries do %&gt;&#xD;
                  &lt;tr class=&lt;%= cycle('', '"even"') %&gt;&gt;&#xD;
                   &lt;% for attribute_name in entry.attributes_names do &#xD;
                         attribute = entry.attributes_hash[attribute_name]&#xD;
                         if attribute.label.is_a?(Array)&#xD;
                              if attribute.respond_to?(:target_context)&#xD;
                                 attr = attribute.label.map{|o| link_to(o[:label], attribute.target_context.url(attribute.target_parameters, o[:target_url]), :class =&gt; attribute_name)}&#xD;
                              else&#xD;
                                attr = attribute.label.map{|o| link_to(o[:label], SHDM::Context.find_by.context_name('OneResource').execute.first.url({}, o[:target_url]), :class =&gt; attribute_name)}&#xD;
                              end&#xD;
                         else                            &#xD;
                           attr = []&#xD;
                           if attribute.respond_to?(:target_url)&#xD;
                             attr &lt;&lt; link_to(attribute.label, attribute.target_url, :rel =&gt; attribute_name)&#xD;
                           else&#xD;
                             attr &lt;&lt; attribute.label&#xD;
                           end&#xD;
                        end %&gt; &#xD;
                      &lt;td&gt;&lt;ul&gt;&lt;% for attribute_value in attr do %&gt;&lt;li style="list-style-type: none"&gt;&lt;%= attribute_value %&gt;&lt;/li&gt;&lt;% end %&gt;&lt;/ul&gt;&lt;/td&gt;&#xD;
                  &lt;% end %&gt;&#xD;
                  &lt;/tr&gt;&#xD;
                &lt;% end %&gt;&#xD;
                &lt;/tbody&gt;&#xD;
              &lt;/table&gt;&#xD;
            &lt;/div&gt;&#xD;
          &lt;/div&gt;&#xD;
        &lt;/div&gt;&#xD;
        &#xD;
      &lt;/div&gt;&#xD;
    &lt;/div&gt;&#xD;
  &lt;/div&gt;&#xD;
&lt;/body&gt;&#xD;
&lt;/html&gt;</swui:abstract_spec>
