    <rdf:type rdf:resource="http://swui#Effect"/>
    <swui:effect_jsCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string">// A wrapper for the Scriptaculous Move effect

if (widgetB == undefined) {
    eval ("new Effect.Move(widgetA, { " + parameters + " })");
  } else {
    var posA = swui_findPos(document.getElementById(widgetA));
    var posB = swui_findPos(document.getElementById(widgetB));
    var x = posA[0]-posB[0];
    var y = posA[1]-posB[1];
    eval ("new Effect.Move(widgetB, {  x: " + x + ", y: " + y + " })");
    $(widgetB).fade({duration: 2.0});
    $(widgetA).appear({duration: 2.0});
  }</swui:effect_jsCode>
    <swui:effect_dependencies rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></swui:effect_dependencies>
    <swui:effect_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">move</swui:effect_name>
  </rdf:Description>
