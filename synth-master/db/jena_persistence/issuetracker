    <swui:abstract_spec rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;div id="landmarks"&gt;&#xD;
   &lt;%= include 'DefaultLandmarksConcrete' %&gt;&#xD;
&lt;/div&gt;&#xD;
---------------------&#xD;
&lt;%= link_to @current_node.previous_node.anchor.label, @current_node.previous_node.anchor.target_url unless @current_node.previous_node.nil? %&gt;&#xD;
&lt;%= link_to @current_node.next_node.anchor.label, @current_node.next_node.anchor.target_url  unless @current_node.next_node.nil? %&gt;&#xD;
---------------------&#xD;
&lt;div id="parameters"&gt;&#xD;
   &lt;% for name, value in @context.parameters do %&gt;&#xD;
       &lt;%= name %&gt;: &lt;%= value.is_a?(RDFS::Resource) ? value.label || value.uri : value %&gt;&#xD;
   &lt;% end %&gt;&#xD;
&lt;/div&gt;&#xD;
---------------------&#xD;
&lt;div id="context_index"&gt;&#xD;
  &lt;% for entry in @context.index.entries do %&gt;&#xD;
    &lt;% for name, attribute in entry.attributes_hash do %&gt;&#xD;
      &lt;% if @current_node.uri == entry.uri %&gt;&#xD;
         &lt;%= attribute.label %&gt;&#xD;
      &lt;% else %&gt;&#xD;
        &lt;%= link_to attribute.label, attribute.target_url %&gt;&#xD;
      &lt;% end %&gt;&#xD;
      &lt;/br&gt;&#xD;
    &lt;% end %&gt;&#xD;
  &lt;% end %&gt;&#xD;
&lt;/div&gt;&#xD;
---------------------&#xD;
&lt;div id="current_node"&gt;&#xD;
  &lt;% for property in @current_node.direct_properties do %&gt;&#xD;
      &lt;% unless property.nil? || property.first.is_a?(RDFS::Resource) %&gt;&#xD;
          &lt;%= property.label.nil? ? property.compact_uri : property.label.first %&gt;: &lt;%= property.to_s %&gt;&lt;/br&gt;&#xD;
      &lt;% end %&gt;&#xD;
  &lt;% end %&gt;&#xD;
  &lt;% for name, attribute in @current_node.attributes_hash do %&gt;&#xD;
      &lt;%= name %&gt;: 		&#xD;
  	&lt;% if attribute.is_a?(IndexNodeAttribute) %&gt;&#xD;
xxxxx&#xD;
&lt;%= attribute.value.entries.size %&gt;&#xD;
	  	&lt;% for entry in attribute.value.entries do %&gt;&#xD;
		    &lt;% for name, attribute in entry.attributes_hash do %&gt;&#xD;
		       &lt;%= link_to attribute.label, attribute.target_url %&gt;&lt;/br&gt;&#xD;
		    &lt;% end %&gt;&#xD;
		  &lt;% end %&gt;&#xD;
xxxxx&#xD;
        &lt;% else %&gt;&#xD;
    	    &lt;% if attribute.respond_to?(:target_url) %&gt;&#xD;
	       &lt;%= link_to attribute.label, attribute.target_url %&gt;&lt;/br&gt;&#xD;
	    &lt;% else %&gt;&#xD;
	       &lt;%= attribute.label %&gt;&lt;/br&gt;&#xD;
	    &lt;% end %&gt;&#xD;
		&lt;% end %&gt;&#xD;
  &lt;% end %&gt;&#xD;
&lt;/div&gt;</swui:abstract_spec>
