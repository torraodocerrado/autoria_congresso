  <rdf:Description rdf:about="http://base#d4d322d0-8139-11df-9021-00264afffe1d">
    <rdf:type rdf:resource="http://swui#Effect"/>
    <swui:effect_jsCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string">// A wrapper for the Scriptaculous Scale effect

   // looks for percent parameter
   var i=0;
   var paramList = parameters;
   var percent = "";
   parameters = "";
   while (true) {
      param = piece(paramList, ",", i, i+1)
      if (param == "") { break; }
      paramName = piece(param, ":", 0, 1);
      if (paramName == "percent") {
         percent = piece(param, ":", 1);
      } else {
         if (parameters != "") { parameters += ", "; }
         parameters += param;
      }
      i++;
   }
   if (percent != "") {
      eval ("new Effect.Scale(widgetA, percent, { " + parameters + " });");
   } else {
      alert ("Percent parameter for Scale effect was not defined!");
   }</swui:effect_jsCode>
    <swui:effect_dependencies rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></swui:effect_dependencies>
    <swui:effect_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">scale</swui:effect_name>
  </rdf:Description>
