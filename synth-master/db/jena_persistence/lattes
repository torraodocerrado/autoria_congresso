  &lt;%= include 'Header' %&gt;&#xD;
    &#xD;
  &lt;%= include 'IndexSideBar' %&gt;&#xD;
			&#xD;
	&lt;div id="contentnorightbar"&gt;&#xD;
		&#xD;
    &lt;% for entry in @index.entries do&#xD;
        attributes = entry.attributes_names.clone&#xD;
        first_name = attributes.delete_at(0)&#xD;
        first_attribute = entry.attributes_hash[first_name] %&gt;  &#xD;
        &lt;div class="entry" id="&lt;%= entry.uri %&gt;"&gt;  &#xD;
        &lt;h2&gt;&lt;%= first_attribute.respond_to?(:target_url) ? link_to(first_attribute.label, (first_attribute.target_url || "#")) : first_attribute.label %&gt;&lt;/h2&gt;&#xD;
        &lt;table&gt;&#xD;
        &lt;tr&gt;&#xD;
          &lt;% for name in attributes do %&gt;&#xD;
            &lt;th&gt;&lt;%= name %&gt;&lt;/th&gt;&#xD;
          &lt;% end %&gt;&#xD;
        &lt;/tr&gt;&#xD;
        &lt;tr&gt;        &#xD;
        &lt;% &#xD;
           for name in attributes do &#xD;
           attribute = entry.attributes_hash[name]&#xD;
           if attribute.label.is_a?(Array)&#xD;
             if attribute.respond_to?(:target_context)&#xD;
                attr = attribute.label.map{|o| link_to(o[:label], attribute.target_context.url((o[:params] || {}), o[:target_url]), :class =&gt; name)}&#xD;
             else&#xD;
                attr = attribute.label.map{|o| link_to(o[:label], o[:target_url], :class =&gt; name)}&#xD;
             end&#xD;
           else                            &#xD;
             if attribute.respond_to?(:target_url)&#xD;
               attr = [ link_to(attribute.label, attribute.target_url, :rel =&gt; name) ]&#xD;
             else&#xD;
               attr = [ attribute.label ]&#xD;
             end&#xD;
           end &#xD;
           %&gt;&#xD;
            &lt;td&gt;&#xD;
              &lt;ul&gt;&#xD;
                &lt;% for attribute_value in attr do %&gt;&lt;li&gt;&lt;%= attribute_value %&gt;&lt;/li&gt;&lt;% end %&gt;&#xD;
              &lt;/ul&gt;&#xD;
   	        &lt;/td&gt;&#xD;
           &#xD;
        &lt;% end %&gt;&#xD;
           &lt;/tr&gt;&#xD;
	       &lt;/table&gt;&#xD;
  	    &lt;/div&gt;&#xD;
  	&lt;% end %&gt;&#xD;
