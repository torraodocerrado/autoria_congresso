  <rdf:Description rdf:about="http://base#3ba86f10-e8f5-11df-bb6f-00264afffe1d">
    <swui:interface_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IndexSideBar</swui:interface_name>
    <swui:widget_decription_type rdf:datatype="http://www.w3.org/2001/XMLSchema#string">concrete</swui:widget_decription_type>
    <swui:abstract_spec rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;div id="sidebar"&gt;&#xD;
  &#xD;
  &lt;h2&gt;&lt;%= @index.index_title %&gt;&lt;/h2&gt;&#xD;
  &#xD;
  &lt;% unless @index.parameters.empty? %&gt;&#xD;
    &lt;h3&gt;Parameters&lt;/h3&gt;	  &#xD;
    &lt;p class="news"&gt;&#xD;
    &lt;% for name, value in @index.parameters do %&gt;&#xD;
       &lt;strong&gt;&lt;%= name %&gt;: &lt;/strong&gt;&lt;%= value.is_a?(RDFS::Resource) ? (value.rdfs::label.first || value.compact_uri) : value %&gt;&#xD;
       &lt;/br&gt;&#xD;
    &lt;% end %&gt;&#xD;
    &lt;/p&gt;&#xD;
  &lt;% end %&gt;&#xD;
&#xD;
  &lt;h3 style="margin: 0;"&gt;Filter&lt;/h3&gt;		&#xD;
  &lt;input id="input_filter" name="" type="text" value="filter" /&gt;&#xD;
&#xD;
&lt;/div&gt;&#xD;
&lt;script&gt;&#xD;
&#xD;
$(document).ready(function(){&#xD;
&#xD;
  $('#input_filter').keyup(function() {&#xD;
     var value = $(this).val().trim();&#xD;
     if(value == ""){&#xD;
       $('.entry').show();            &#xD;
     }&#xD;
     else&#xD;
    {&#xD;
      var re = new RegExp(value,"i");&#xD;
      $('.entry').hide()&#xD;
      $('.entry').filter(function() {&#xD;
        return re.test($(this).find('h2').text()) &#xD;
      }).show();&#xD;
      }     &#xD;
  });&#xD;
 });&#xD;
&#xD;
&lt;/script&gt;</swui:abstract_spec>
    <swui:rhet_spec rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></swui:rhet_spec>
    <rdf:type rdf:resource="http://swui#ComponentInterface"/>
  </rdf:Description>
