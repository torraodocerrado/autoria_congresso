  <rdf:Description rdf:about="http://base#773c8860-6b6c-11df-8c3d-00264afffe1d">
    <rdf:type rdf:resource="http://swui#ComponentInterface"/>
    <swui:dynamic rdf:datatype="http://www.w3.org/2001/XMLSchema#string">true</swui:dynamic>
    <swui:rhet_spec rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></swui:rhet_spec>
    <swui:abstract_spec rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;%&#xD;
	    ModelUtils.traverseJSON(jsonData,list) do |type,key,value,hash,list| &#xD;
              if !(key.include? "swui:")&#xD;
                name = key&#xD;
                name = name.gsub(/\|/,"-")&#xD;
                #name = name.gsub(/_/,"-")&#xD;
                if (type=="Item")&#xD;
                    buffer &lt;&lt; "&lt;CompositeInterfaceElement mapsTo=\"Composition\" name=\"#{name}Attr\" &gt;" &#xD;
		    buffer &lt;&lt; "&lt;ElementExhibitor name=\"#{name}Lbl\" mapsTo=\"Text\" defaultContent=\"#{name}: \" cssClasses=\"swui_bold\"/&gt;" if !list&#xD;
	            if (hash.has_key? "#{key}|swui:target")&#xD;
		      buffer &lt;&lt; "&lt;SimpleActivator mapsTo=\"Link\" name=\"#{name}Link\" fromAttribute=\"#{key}|swui:target\"&gt;" &#xD;
	            end&#xD;
                    widget = "&lt;ElementExhibitor mapsTo=\"Text\" name=\"#{name}\" fromAttribute=\"#{key}\"/&gt;" &#xD;
		    widget.to_s.gsub!(/mapsTo="Text"/,"mapsTo=\"Image\"") if (value.to_s.match(/.+\.(gif|jpeg|jpg|png|tiff)$/))&#xD;
                    buffer &lt;&lt; widget&#xD;
	            if (hash.has_key? "#{key}|swui:target")&#xD;
                      buffer &lt;&lt; "&lt;/SimpleActivator&gt;"&#xD;
	            end&#xD;
                    buffer &lt;&lt; "&lt;/CompositeInterfaceElement&gt;" &#xD;
                end&#xD;
                if (type=="OpenList")&#xD;
	          buffer &lt;&lt; "&lt;CompositeInterfaceElement mapsTo=\"Composition\" name=\"#{name}List\" fromAttribute=\"#{key}\" cssClasses=\"swui_composite\" ordered=\"true\"&gt;" &#xD;
		  buffer &lt;&lt; "&lt;ElementExhibitor name=\"#{name}ListLbl\" mapsTo=\"Text\" defaultContent=\"#{name}: \" cssClasses=\"swui_bold\"/&gt;"&#xD;
                  buffer &lt;&lt; "&lt;CompositeInterfaceElement mapsTo=\"Composition\" name=\"#{name}\" fromClass=\"#{name}\" &gt;"&#xD;
                end&#xD;
	        buffer &lt;&lt; "&lt;/CompositeInterfaceElement&gt; &lt;/CompositeInterfaceElement&gt;" if (type=="CloseList")&#xD;
              end&#xD;
	    end&#xD;
%&gt;</swui:abstract_spec>
    <swui:interface_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">DefaultJSONDisplay</swui:interface_name>
